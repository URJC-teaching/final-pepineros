<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="3"
      main_tree_to_execute="BehaviorTree">
  <BehaviorTree ID="BehaviorTree">
    <Repeat num_cycles="5">
      <Sequence>

        <!--Primer Waypoint-->
        <GetWaypoint x="-5.31"
                     y="-3.9"
                     yaw="-1.5708"
                     waypoint="{wp}"/>
        <Move goal="{wp}"/>

        <!--Segundo Waypoint-->
        <GetWaypoint x="-4.46"
                     y="-6.47"
                     yaw="0.0"
                     waypoint="{wp}"/>
        <Move goal="{wp}"/>

        <!--Tercer Waypoint-->
        <GetWaypoint x="-0.26"
                     y="-5.98"
                     yaw="1.5708"
                     waypoint="{wp}"/>
        <Move goal="{wp}"/>

        <!--Cuarto Waypoint-->
        <GetWaypoint x="-0.551"
                     y="-2.39"
                     yaw="3.14"
                     waypoint="{wp}"/>
        <Move goal="{wp}"/>
        
      </Sequence>
    </Repeat>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="GetWaypoint">
      <input_port name="x"
                  default="1.0"/>
      <input_port name="y"
                  default="1.0"/>
      <input_port name="yaw"
                  default="1.0"/>
      <output_port name="waypoint"/>
    </Action>
    <Action ID="Move">
      <input_port name="goal"/>
    </Action>
  </TreeNodesModel>

</root>
